{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier", "react-hooks"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off"
  },
  "globals": {
    "test": true,
    "expect": true,
    "afterEach": true,
    "FB": true,
    "describe": true,
    "it": true,
    "gapi": true,
    "cy": true
  }
}
