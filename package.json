{
  "name": "wg-fe-ui",
  "version": "0.9.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/* && babel src -d dist --copy-files",
    "test": "jest",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-info": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-alpha.20",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.0.4",
    "detect-browser": "^4.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "formik": "^1.5.8",
    "i18next": "^17.2.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.18.2",
    "react-calendar": "^2.19.2",
    "react-dom": "^16.12.0",
    "react-drag-drawer": "^3.3.1",
    "react-input-mask": "^2.0.4",
    "react-router": "^5.1.2",
    "react-scripts": "^3.1.2",
    "react-select": "^3.0.8",
    "rimraf": "^3.0.0",
    "styled-components": "^4.3.2",
    "styled-theme": "^0.3.3",
    "stylelint": "^10.1.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.7",
    "yup": "^0.27.0"
  },
  "devDependencies": {}
}
